{
  "name": "@superstreamer/api",
  "version": "1.1.0",
  "type": "module",
  "exports": {
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    }
  },
  "scripts": {
    "dev": "bun run ./scripts/dev.ts",
    "build": "bun run ./scripts/build.ts",
    "lint": "tsc && eslint"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/pg": "^8.11.10",
    "config": "workspace:*",
    "eslint": "^9.14.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.685.0",
    "@aws-sdk/s3-request-presigner": "^3.685.0",
    "@elysiajs/bearer": "^1.1.2",
    "@elysiajs/cors": "^1.1.1",
    "@elysiajs/eden": "^1.1.3",
    "@elysiajs/jwt": "1.1.0",
    "@hono/zod-validator": "^0.4.2",
    "@matvp91/elysia-swagger": "^2.0.0",
    "bolt": "workspace:*",
    "bullmq": "^5.25.0",
    "elysia": "^1.1.24",
    "hono": "^4.7.0",
    "hono-openapi": "^0.4.4",
    "kysely": "^0.27.4",
    "pg": "^8.13.1",
    "shared": "workspace:*",
    "zod": "^3.24.2",
    "zod-openapi": "^4.2.3"
  },
  "tsup": {
    "entry": {
      "client": "./client/index.ts"
